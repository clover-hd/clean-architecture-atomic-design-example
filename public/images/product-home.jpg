  <!-- Windows -->
  <rect x="100" y="145" width="20" height="20" fill="#93c5fd" stroke="#1e40af" stroke-width="2"/>
  <line x1="110" y1="145" x2="110" y2="165" stroke="#1e40af" stroke-width="1"/>
  <line x1="100" y1="155" x2="120" y2="155" stroke="#1e40af" stroke-width="1"/>

  <rect x="180" y="145" width="20" height="20" fill="#93c5fd" stroke="#1e40af" stroke-width="2"/>
