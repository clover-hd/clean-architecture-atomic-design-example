  <rect x="190" y="70" width="15" height="30" fill="#6b7280"/>
  <rect x="188" y="68" width="19" height="8" fill="#4b5563"/>

  <!-- Smoke -->
  <circle cx="200" cy="60" r="3" fill="#d1d5db" opacity="0.6"/>
  <circle cx="205" cy="55" r="2" fill="#d1d5db" opacity="0.4"/>
  <circle cx="202" cy="50" r="2" fill="#d1d5db" opacity="0.3"/>

  <!-- Garden elements -->
  <circle cx="70" cy="220" r="15" fill="#84cc16"/>
  <rect x="67" y="215" width="6" height="10" fill="#65a30d"/>

  <circle cx="230" cy="220" r="15" fill="#84cc16"/>
  <rect x="227" y="215" width="6" height="10" fill="#65a30d"/>

  <!-- Decorative fence -->
  <rect x="40" y="230" width="220" height="3" fill="#8b5cf6"/>
  <rect x="50" y="225" width="3" height="15" fill="#8b5cf6"/>
  <rect x="70" y="225" width="3" height="15" fill="#8b5cf6"/>
  <rect x="90" y="225" width="3" height="15" fill="#8b5cf6"/>
  <rect x="210" y="225" width="3" height="15" fill="#8b5cf6"/>
  <rect x="230" y="225" width="3" height="15" fill="#8b5cf6"/>
  <rect x="250" y="225" width="3" height="15" fill="#8b5cf6"/>

  <!-- Text -->
  <text x="150" y="265" text-anchor="middle" fill="#059669" font-family="Arial, sans-serif" font-size="16" font-weight="600">Home & Garden</text>
  <text x="150" y="280" text-anchor="middle" fill="#065f46" font-family="Arial, sans-serif" font-size="12">ホーム・ガーデン</text>
</svg>