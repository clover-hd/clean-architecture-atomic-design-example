<svg width="300" height="300" viewBox="0 0 300 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <defs>
    <linearGradient id="homeGrad" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#ecfdf5;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f8fafc;stop-opacity:1" />
    </linearGradient>
  </defs>
  <rect width="300" height="300" fill="url(#homeGrad)"/>

  <!-- House structure -->
  <path d="M150 60 L220 120 L220 200 L80 200 L80 120 Z" fill="#065f46"/>
  <rect x="90" y="130" width="120" height="70" fill="#10b981"/>

